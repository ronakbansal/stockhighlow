<div data-role="header">
  <h1>Historical Data</h1>
  <%= link_to "Full Site", :mobile => 0, :class => "ui-btn-right" %>
</div>
<div data-role="content">
<table data-role="table" id="table-column-toggle" class="ui-responsive table-stroke">
<thead>
<tr>
<th>Date</th>
<th>Open</th>
<th>High</th>
<th>Low</th>

</tr>
</thead>
<tbody>
<% @csv.each do |row| %>
<tr>
<td><%= row[0] %></td>
<td><%= row[1] %></td>
<td><%= row[2] %></td>
<td><%= row[3] %></td>

</tr>
<% end %>
</tbody>
</table>
</div>