<h1>Required data :</h1>
<table cellpadding="0" cellspacing="0" border="1" class="display" id="table_table" width="100%">
<thead>
<tr>
<th>Date</th>
<th>Open</th>
<th>High</th>
<th>Low</th>
<th>Close</th>
<th>Volume</th>
<th>Adj. Close</th>
</tr>
</thead>
<tbody>
<% @csv.each do |row| %>
<tr>
<td><%= row[0] %></td>
<td><%= row[1] %></td>
<td><%= row[2] %></td>
<td><%= row[3] %></td>
<td><%= row[4] %></td>
<td><%= row[5] %></td>
<td><%= row[6] %></td>
</tr>
<% end %>
</tbody>
</table>
